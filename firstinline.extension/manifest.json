{
  "manifest_version": 2,

  "name": "First In Line",
  "description": "You choose the shoe, we'll seal the deal for you.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://firstinline.me",
    "self",
    "https://code.jquery.com"
  ],
  "content_scripts": [
    {
      "js" : ["js.js", "*.js"]

    }
  ],
  "content_security_policy": "https://code.jquery.com http://firstinline.me"
}
